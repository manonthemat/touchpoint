  <nav>
    <% if current_user %>
      <%= link_to "My Dashboard", user_path(current_user.id) %>
      Welcome, <%= current_user.fname %>!
      (id: <%= current_user.id %>)
        <%= link_to "Log out", session_path, method: :delete %>
      <%else %>
        <%= link_to "Sign up", new_user_path if !@is_signup %>
        <%= link_to "Log in", new_session_path if !@is_login %>
    <% end %>
  </nav>