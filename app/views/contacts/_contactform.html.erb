<%= form_for @contact do |f| %>
  <%= f.text_field :fname, placeholder: "first name" %>
  <%= f.text_field :lname, placeholder: "last name" %>
  <%= f.text_field :spouse, placeholder: "spouse" %>
  <%= f.text_field :email, placeholder: "email address" %>
  <%= f.text_field :home_phone, placeholder: "home phone" %>
  <%= f.text_field :mobile_phone, placeholder: "mobile phone" %>
  <%= f.text_field :address, placeholder: "address" %>
  <%= f.text_field :giving_level, placeholder: "giving level" %>
  <%= f.submit "Submit!" %>
<% end %>